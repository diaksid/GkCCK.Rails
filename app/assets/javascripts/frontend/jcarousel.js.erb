(function ($, PRO) {
  PRO.carousel = function () {
    var obj = $('[data-pro-jcarousel]')
    if (obj.length) {
      var fn = function () {
        obj.proJcarousel()
      }
      if ($.jcarousel) {
        fn()
      } else {
        $.script('<%= asset_path('carousel.js') %>', fn)
      }
    }
    return this
  }
})($, PRO)
